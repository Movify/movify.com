
{# Default #}

{% set defaults = {
  form: block.form,
  title: block.titleContent,
  subtitle: block.titleSubtitle,
  position: block.positionContact,
} %}


<div class="container">
  <div class="row">
    <div class="col-12 col-md-4 {{options.position}}">
      <h2>
        {{block.titleContent}}
      </h2>
      <p>
        {{block.titleSubtitle}}
      </p>
      <div class="e-spacer-2 d-md-none"></div>
    </div>
    <div class="col-12 col-md-6 offset-md-2">
      {# form contact #}
      {% set formOptions = {
        formId: options.form,
      } %}
      {% include 'forms/contact.twig' with { options: formOptions } %}
    </div>
  </div>
</div>

{% set options = null %}
