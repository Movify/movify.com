


{% for block in entry.page.level(1).all() %}

{% if loop.first %}
    {% set first = true %}
{% endif %}


{% switch block.type.handle %}

{# SLIDERS #}



{# sliderMembers #}
{% case 'sliderMembers' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="u-bg-white {{navTargetClass}}" {{navTarget}}>
  <div class="container">
  {% for block in block.sliderTeam.all() %}
    {# slidercard #}
    {% set slidercardOptions = {
      content: block.intro,
      position: block.position,
      items: block.members,
    } %}
    {% include '_components/slidercard.twig' with { options: slidercardOptions } %}
  {% endfor %}
  </div>
</section>

{# sliderBullet #}
{% case 'sliderBullet' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="decoration__section u-bg-white {{navTargetClass}}" {{navTarget}}>
  {# decoration #}
  {% set decorationOptions = {
    template: block.decorationTemplate,
  } %}
  {% include '_components/decoration.twig' with { options: decorationOptions } %}
  <div class="e-spacer-section"></div>
  <div class="container">
    {# button #}
    {% set sliderBulletOptions = {
      title:block.titleContent,
      items:block.sliderBullet,
      button:block.button,
      active:'0'
    } %}
    {% include '_components/sliderbullet.twig' with { options: sliderBulletOptions } %}
  </div>
  <div class="e-spacer-section"></div>
</section>

{# sliderContent #}
{% case 'sliderContent' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="u-bg-gray1 decoration__section {{navTargetClass}}" {{navTarget}}>
  {# decoration #}
  {% set decorationOptions = {
    template: block.decorationTemplate,
  } %}
  {% include '_components/decoration.twig' with { options: decorationOptions } %}
  <div class="e-spacer-section"></div>
  <div class="container">
      {# slidercontent #}
      {% set sliderContentOptions = {
        title: block.titleContent,
        items: block.sliderContent,
      } %}
      {% include '_components/slidercontent.twig' with { options: sliderContentOptions } %}
  </div>
  <div class="e-spacer-section"></div>
</section>

{# CONTACT #}

{# contact #}
{% case 'contact' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}
{% set mode = macros.negative(block.mode) %}
{% if block.background %} {% set background = block.background %}{% endif %}

<section class="{{background}} {{navTargetClass}} {{mode}} decoration__section" {{navTarget}}>
  {# decoration #}
  {% set decorationOptions = {
    template: block.decorationTemplate,
  } %}
  {% include '_components/decoration.twig' with { options: decorationOptions } %}
  <div class="e-spacer-section"></div>
  {# contact #}
  {% set contactOptions = {
    form: block.form,
    title: block.titleContent,
    subtitle: block.titleSubtitle,
    position: block.positionContact,
  } %}
  {% include '_components/contact.twig' with { options: contactOptions } %}
  <div class="e-spacer-section"></div>
</section>

{# office #}
{% case 'office' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="u-bg-white decoration__section {{navTargetClass}}" {{navTarget}}>
  {# decoration #}
  {% set decorationOptions = {
    template: block.decorationTemplate,
  } %}
  {% include '_components/decoration.twig' with { options: decorationOptions } %}
  {% if first %}
    <div class="e-spacer-section"></div>
    {% set first = false %}
  {% endif %}
  <div class="container">
    <div class="row">
      <div class="col-12">
        <h3 class="p-contact-infotitle">
          {{block.titleContent}}
        </h3>
        <div class="e-spacer-1"></div>
      </div>
      {% for row in block.office.all() %}
        <div class="col-12 col-md-4 col-lg-3">
          <h4>{{row.officeInfo}}</h4>
          <p>
            {{row.officeDetails | nl2br}}
          </p>
          <div class="e-spacer-2 d-md-none"></div>
        </div>
      {% endfor %}
    </div>
  </div>
  <div class="e-spacer-section"></div>
</section>

{# map #}
{% case 'map' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="{{navTargetClass}}" {{navTarget}}>
  {# map #}
  {% set mapOptions = {
    items: block.map,
  } %}
  {% include '_components/map.twig' with { options: mapOptions } %}
</section>

{# social #}
{% case 'social' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="u-bg-white {{navTargetClass}}" {{navTarget}}>
  <div class="container">
    <div class="row">
      <div class="col-12 col-md-6 col-lg-4">
        <div class="e-card u-bg-gray1">
          <h3 class="p-contact-infotitle">
            Our social channels
          </h3>
          <ul class="e-list-social e-list-social--left">
            {% for row in informations.social.all() %}
                <li>
                  <a href="{{ row.socialUrl }}" target="_blank">
                    <span class="icon-movify-iconfont-{{ row.socialIcon }}"></span>
                  </a>
                </li>
            {% endfor %}
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="e-spacer-section"></div>
</section>

