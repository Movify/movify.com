{# blocs #}
{% case 'blocs' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="c-blocs {{navTargetClass}}" {{navTarget}}>
  {% for block in block.blocs.all() %}
    {% set blocsOptions = {
      title: block.blocTitle,
      content: block.blocContent,
      color: block.blocColor,
      direction: block.blocDirection,
      image: block.blocImage,
    } %}
    {% include '_components/blocs.twig' with { options: blocsOptions } %}
  {% endfor %}
</section>

{# expand #}
{% case 'expand' %}

{% import 'macros/helpers' as macros %}
{% set navTarget = macros.target(block.navTarget) %}
{% set navTargetClass = macros.targetClass(block.navTarget) %}

<section class="{{navTargetClass}} u-mode-negative" {{navTarget}}>
  {# decoration #}
  {% set decorationOptions = {
    template: block.decorationTemplate,
  } %}
  {% include '_components/decoration.twig' with { options: decorationOptions } %}
  <div class="e-spacer-section"></div>
  <div class="container">
    {% if block.titleContent != '' %}
      <div class="row">
        <div class="col-12">
          <h2>{{block.titleContent}}</h2>
        </div>
      </div>
    {% endif %}
    <div class="row">
      <div class="col-12">
      {% set blocsOptions = {
        items: block.expand,
      } %}
      {% include '_components/expand.twig' with { options: blocsOptions } %}
      </div>
    </div>
  </div>
  <div class="e-spacer-section"></div>
</section>

{# navPage #}
{% case 'navPage' %}

<section class="u-bg-brand2">
  <div class="container">
    <div class="row">
      <div class="col-12">
        {% set navPageOptions = {
          items:block.navPage,
        } %}
        {% include '_components/navpage.twig' with { options: navPageOptions } %}
      </div>
    </div>
  </div>
  <div class="e-spacer-1"></div>
</section>

{# CUSTOM #}

{# navPage #}
