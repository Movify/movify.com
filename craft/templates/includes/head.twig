<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta http-equiv="X-UA-Compatible" content="ie=edge">
<link rel="stylesheet" href="/assets/css/movify.css">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

{% if entry.title == true %}
  <meta name="twitter:title" content="{{ title }} - {{ siteName }}">
  <meta property="og:title" content="{{ title }} - {{ siteName }}">
{% endif %}

{% set seoKeywords = entry.seoKeywords.all() %}
{% if seoKeywords|length %}
      <meta name="keywords" content="{% for rel in seoKeywords %}{{rel.title}}, {% endfor %}">
{% endif %}

{% if entry.seoDescription == true %}
  <meta name="description" content="{{entry.seoDescription}}">
  <meta name="twitter:description" content="{{entry.seoDescription}}">
  <meta property="og:description" content="{{entry.seoDescription}}">
{% endif %}

{% set image = entry.seoImage.one() %}
{% if image %}
  {% set seoImage = image.url %}
  <meta property="og:image" content="{{seoImage}}">
  <meta name="twitter:image" content="{{seoImage}}">
{% endif %}

